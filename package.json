{
  "name": "wadv-stock",
  "version": "1.0.3",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "node scripts/version-bump.js && npm run build && electron-builder --dir",
    "build:win": "node scripts/version-bump.js && npm run build && electron-builder --win",
    "build:mac": "node scripts/version-bump.js && npm run build && electron-builder --mac",
    "build:linux": "node scripts/version-bump.js && npm run build && electron-builder --linux",
    "build:icons": "npx electron-icon-builder --input=./resources/icon.png --output=build --flatten",
    "release": "release-it"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@tailwindcss/vite": "^4.1.15",
    "@vueuse/core": "^13.9.0",
    "ag-grid-community": "^35.0.0",
    "ag-grid-vue3": "^35.0.0",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-store": "8.2.0",
    "electron-updater": "^6.3.9",
    "klinecharts": "10.0.0-alpha5",
    "lucide-vue-next": "^0.546.0",
    "mitt": "^3.0.1",
    "pinia": "^3.0.3",
    "reka-ui": "^2.6.0",
    "semver": "^7.7.3",
    "sortablejs": "^1.15.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.15",
    "topbar": "^3.0.0",
    "tw-animate-css": "^1.4.0",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3",
    "vue-sonner": "^2.0.9"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@types/node": "^22.18.6",
    "@types/semver": "^7.7.1",
    "@types/sortablejs": "^1.15.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^38.1.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.1",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vue": "^3.5.21",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild",
      "klinecharts"
    ]
  },
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d"
}
